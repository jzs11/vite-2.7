{
  "liveServer.settings.donotShowInfoMsg": true,
  "[vue]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[typescript]": {},
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "[json]": {},
  "editor.tabSize": 2,
  "[javascript]": {},
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.autofetch": true,
  "[scss]": {},
  "workbench.colorTheme": "Dracula",
  "[css]": {},
  "files.exclude": {
    "**/build": true,
    "**/dist": true,
    "**/node_modules": true,
    "public/**/*.css": true
  },
  "search.exclude": {
    "**/dist": true,
    "**/build": true
  },
  "search.useGlobalIgnoreFiles": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.codeLens": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": false
  },
  "editor.formatOnSave": true,

  "files.eol": "\n",
  "cSpell.words": [
    "accordiontab",
    "aspnet",
    "autofocus",
    "avatargroup",
    "backface",
    "badgedirective",
    "borderless",
    "Bson",
    "calibri",
    "Chevrondown",
    "colorpicker",
    "confirmationservice",
    "confirmdialog",
    "confirmpopup",
    "content",
    "datagrid",
    "datatable",
    "dataview",
    "dataviewlayoutoptions",
    "devextreme",
    "dismissable",
    "draggable",
    "dx",
    "field",
    "fileupload",
    "fileuploader",
    "focusable",
    "fontawesome",
    "fortawesome",
    "fullcalendar",
    "Grav",
    "icon",
    "inlinemessage",
    "Inplace",
    "inputmask",
    "inputnumber",
    "inputswitch",
    "inputtext",
    "keydown",
    "keyframes",
    "Listbox",
    "megamenu",
    "mouseleave",
    "multiselect",
    "nojquery",
    "nowrap",
    "orderlist",
    "organizationchart",
    "outter",
    "overlaypanel",
    "panelmenu",
    "picklist",
    "Pinia",
    "plusplus",
    "primeflex",
    "primeicons",
